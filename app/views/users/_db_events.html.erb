<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header">
        <h3 class="card-title">
          Upcoming Events You're Following<br />
          <p class="category"><%= link_to 'See all events', events_path %></p>
        </h3>
      </div>
      <div class="card-content">
        <% @user.followed_events.each do |event| %>
          <%= render partial: 'events/event', object: event %>
        <% end %>
      </div>
    </div>
  </div>
</div>
